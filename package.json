{
  "name": "homeless",
  "version": "1.2.1",
  "description": "The first step to a bright react future.",
  "main": "build/server/index.js",
  "engines": {
    "node": ">=6"
  },
  "scripts": {
    "analyze:client": "babel-node internal/scripts/analyze --client",
    "analyze:server": "babel-node internal/scripts/analyze --server",
    "build": "babel-node internal/scripts/build --optimize",
    "build:cordova": "cross-env CORDOVA=true babel-node internal/scripts/build --optimize",
    "build:dev": "babel-node internal/scripts/build",
    "clean": "cross-env babel-node internal/scripts/clean",
    "deploy": "babel-node internal/scripts/deploy",
    "develop": "cross-env DEPLOYMENT=development babel-node internal/development",
    "develop:cordova": "cross-env CORDOVA=true DEPLOYMENT=development babel-node internal/development",
    "lint": "eslint src config internal",
    "precommit": "lint-staged",
    "preinstall": "node internal/scripts/preinstall",
    "prepush": "jest",
    "start": "cross-env NODE_ENV=production nodemon build/server",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "e2e": "node ./internal/scripts/selenium-download.js && sh ./internal/scripts/integration-tests.sh"
  },
  "lint-staged": {
    "*.js": [
      "prettier-eslint --single-quote --trailing-comma es5 --write",
      "git add"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "testResultsProcessor": "jest-teamcity-reporter",
    "snapshotSerializers": [
      "<rootDir>/node_modules/enzyme-to-json/serializer"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/(build|internal|node_modules|public)/"
    ],
    "moduleNameMapper": {
      "\\.(css|less|sass|scss|styl)$": "identity-obj-proxy",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/internal/jest/assetMock.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ctrlplusb/react-universally.git"
  },
  "keywords": [
    "react",
    "boilerplate",
    "starter kit",
    "universal",
    "javascript",
    "express",
    "webpack"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ctrlplusb/react-universally/issues"
  },
  "homepage": "https://github.com/ctrlplusb/react-universally#readme",
  "dependencies": {
    "app-root-dir": "1.0.2",
    "axios": "0.16.1",
    "cancelable-promise": "^2.4.0",
    "classnames": "^2.2.5",
    "colors": "1.1.2",
    "compression": "1.6.2",
    "cross-env": "4.0.0",
    "css-element-queries": "^0.4.0",
    "dotenv": "4.0.0",
    "express": "4.15.2",
    "express-interceptor": "^1.2.0",
    "helmet": "3.5.0",
    "hpp": "0.2.2",
    "identity-obj-proxy": "^3.0.0",
    "mobile-detect": "^1.3.6",
    "modernizr": "3.5.0",
    "muicss": "^0.9.27",
    "nodemon": "^1.12.1",
    "normalize.css": "6.0.0",
    "offline-plugin": "4.7.0",
    "polylabel": "^1.0.2",
    "prop-types": "15.5.8",
    "ptsans-npm-webfont": "^0.0.4",
    "qs": "^6.5.1",
    "react": "15.5.4",
    "react-async-bootstrapper": "1.1.1",
    "react-async-component": "1.0.2",
    "react-copy-to-clipboard": "^5.0.0",
    "react-dom": "15.5.4",
    "react-helmet": "5.0.3",
    "react-id-swiper": "1.4.0",
    "react-ink": "^6.2.0",
    "react-input-mask": "^1.0.7",
    "react-input-range": "^1.2.1",
    "react-jobs": "1.0.0-beta.3",
    "react-md-spinner": "^0.2.5",
    "react-modal": "^2.2.4",
    "react-redux": "5.0.4",
    "react-router-dom": "4.1.1",
    "react-scroll": "^1.6.1",
    "react-svg-pan-zoom": "^2.13.0",
    "react-swipeable": "^4.1.0",
    "react-tap-event-plugin": "^2.0.1",
    "react-transition-group": "^2.2.1",
    "react-window-size": "^1.0.1",
    "redux": "3.6.0",
    "redux-thunk": "2.2.0",
